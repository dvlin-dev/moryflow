{
  "name": "aiget",
  "private": true,
  "scripts": {
    "dev:server": "pnpm --filter @aiget/server start:dev",
    "dev:console": "pnpm --filter @aiget/console dev",
    "dev:docs": "pnpm --filter @aiget/docs dev",
    "build:server": "pnpm --filter @aiget/server build",
    "build:console": "pnpm --filter @aiget/console build",
    "build:docs": "pnpm --filter @aiget/docs build",
    "build": "pnpm run build:server && pnpm run build:console",
    "deploy:docs": "pnpm --filter @aiget/docs deploy",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "typescript": "^5.9.3"
  }
}
