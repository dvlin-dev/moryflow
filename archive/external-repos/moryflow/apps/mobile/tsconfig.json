{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@moryflow/shared-storage/*": ["../../packages/shared-storage/src/*"],
      "@moryflow/shared-types/*": ["../../packages/shared-types/src/*"],
      "@moryflow/shared-core/*": ["../../packages/shared-core/src/*"],
      "@moryflow/shared-types": ["../../packages/shared-types/src/index.ts"],
      "@moryflow/shared-core": ["../../packages/shared-core/src/index.ts"],
      "@moryflow/tiptap/*": ["../../packages/tiptap/src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "uniwind-types.d.ts"]
}
