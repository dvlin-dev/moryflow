  },
  "dependencies": {
    "@moryflow/agents": "workspace:*",
    "@moryflow/agents-core": "workspace:*",
    "zod": "^3.25.40"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
