{
  "name": "@moryflow/model-registry-data",
  "version": "0.1.0",
  "description": "LiteLLM 模型数据注册表，提供模型搜索和自动填充功能",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "sync": "tsx scripts/sync.ts",
    "prebuild": "pnpm sync",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
