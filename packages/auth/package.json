{
  "name": "@aiget/auth",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc-multi",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "better-auth": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aiget/eslint-config": "workspace:*",
    "@aiget/typescript-config": "workspace:*",
    "@types/react": "^19.2.5",
    "tsc-multi": "^1.1.0",
    "typescript": "~5.9.3"
  }
}
