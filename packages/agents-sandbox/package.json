  },
  "dependencies": {
    "@openai/agents-core": "0.4.3",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
