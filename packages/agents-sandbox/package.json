{
  "name": "@anyhunt/agents-sandbox",
  "version": "0.1.0",
  "description": "Local sandbox execution for Moryflow Agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
