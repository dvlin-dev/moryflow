{
  "name": "@moryflow/agents-runtime",
  "version": "0.1.0",
  "description": "Core runtime for Moryflow Agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./prompt": {
      "types": "./dist/prompt.d.ts",
      "require": "./dist/prompt.js",
      "import": "./dist/prompt.mjs"
    },
    "./ui-message/visibility-policy": {
      "types": "./dist/ui-message/visibility-policy.d.ts",
      "require": "./dist/ui-message/visibility-policy.js",
      "import": "./dist/ui-message/visibility-policy.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@moryflow/agents-adapter": "workspace:*",
    "@moryflow/api": "workspace:*",
    "@moryflow/model-bank": "workspace:*",
    "@openai/agents-core": "0.5.1",
    "@openai/agents-extensions": "0.5.1",
    "@ai-sdk/openai": "3.0.31",
    "@ai-sdk/anthropic": "3.0.46",
    "@ai-sdk/google": "3.0.30",
    "@ai-sdk/xai": "3.0.57",
    "@ai-sdk/openai-compatible": "2.0.30",
    "@ai-sdk/provider": "3.0.8",
    "@openrouter/ai-sdk-provider": "2.2.3",
    "ai": "6.0.97",
    "jsonc-parser": "^3.3.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
