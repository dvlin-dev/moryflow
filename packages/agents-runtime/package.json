{
  "name": "@anyhunt/agents-runtime",
  "version": "0.1.0",
  "description": "Core runtime for Moryflow Agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./prompt": {
      "types": "./dist/prompt.d.ts",
      "require": "./dist/prompt.js",
      "import": "./dist/prompt.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@anyhunt/agents-adapter": "workspace:*",
    "@anyhunt/api": "workspace:*",
    "@openai/agents-core": "0.4.3",
    "@openai/agents-extensions": "0.4.3",
    "@ai-sdk/openai": "3.0.18",
    "@ai-sdk/anthropic": "3.0.23",
    "@ai-sdk/google": "3.0.13",
    "@ai-sdk/xai": "3.0.34",
    "@ai-sdk/openai-compatible": "2.0.18",
    "@ai-sdk/provider": "3.0.5",
    "@openrouter/ai-sdk-provider": "2.0.1",
    "jsonc-parser": "^3.3.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
