{
  "name": "@aiget/api",
  "version": "1.0.0",
  "description": "共享 API 类型和常量，供 PC 和 Mobile 共用",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.js",
      "import": "./dist/client/index.mjs"
    },
    "./cloud-sync": {
      "types": "./dist/cloud-sync/index.d.ts",
      "require": "./dist/cloud-sync/index.js",
      "import": "./dist/cloud-sync/index.mjs"
    }
  },
  "files": ["dist"],
  "license": "MIT",
  "scripts": {
    "build": "tsc-multi",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aiget/sync": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
