{
  "name": "@aiget/identity-db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./generated/prisma/client.ts",
  "types": "./generated/prisma/client.ts",
  "exports": {
    ".": {
      "types": "./generated/prisma/client.ts",
      "import": "./generated/prisma/client.ts",
      "require": "./generated/prisma/client.ts"
    }
  },
  "files": [
    "generated"
  ],
  "scripts": {
    "build": "IDENTITY_DATABASE_URL=postgresql://dummy:dummy@localhost:5432/dummy prisma generate --schema=prisma/schema.prisma",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf generated",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^7.1.0"
  },
  "devDependencies": {
    "@aiget/eslint-config": "workspace:*",
    "dotenv": "^17.2.3",
    "prisma": "^7.1.0"
  }
}
