{
  "name": "@anyhunt/auth-server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc-multi",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anyhunt/identity-db": "workspace:*",
    "better-auth": "^1.4.5",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  },
  "devDependencies": {
    "@anyhunt/eslint-config": "workspace:*",
    "@anyhunt/typescript-config": "workspace:*",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@types/node": "^22.10.7",
    "nestjs-zod": "^5.0.1",
    "tsc-multi": "^1.1.0",
    "typescript": "~5.9.3"
  }
}
