{
  "name": "@moryflow/model-bank",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./aiModels/*": {
      "types": "./dist/aiModels/*.d.ts",
      "require": "./dist/aiModels/*.cjs",
      "import": "./dist/aiModels/*.mjs"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "require": "./dist/registry/index.cjs",
      "import": "./dist/registry/index.mjs"
    },
    "./registry/*": {
      "types": "./dist/registry/*.d.ts",
      "require": "./dist/registry/*.cjs",
      "import": "./dist/registry/*.mjs"
    },
    "./thinking": {
      "types": "./dist/thinking/index.d.ts",
      "require": "./dist/thinking/index.cjs",
      "import": "./dist/thinking/index.mjs"
    },
    "./thinking/*": {
      "types": "./dist/thinking/*.d.ts",
      "require": "./dist/thinking/*.cjs",
      "import": "./dist/thinking/*.mjs"
    },
    "./modelProviders": {
      "types": "./dist/modelProviders/index.d.ts",
      "require": "./dist/modelProviders/index.cjs",
      "import": "./dist/modelProviders/index.mjs"
    },
    "./modelProviders/*": {
      "types": "./dist/modelProviders/*.d.ts",
      "require": "./dist/modelProviders/*.cjs",
      "import": "./dist/modelProviders/*.mjs"
    },
    "./modelProviders/moryflow": {
      "types": "./dist/modelProviders/moryflow.d.ts",
      "require": "./dist/modelProviders/moryflow.cjs",
      "import": "./dist/modelProviders/moryflow.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/types/index.cjs",
      "import": "./dist/types/index.mjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/types/*.cjs",
      "import": "./dist/types/*.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc-multi",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage --silent='passed-only'",
    "test:unit": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "type-fest": "^5.2.0",
    "zod": "^3.25.76"
  },
  "description": "Model/provider registry and model-native parameter schema bank",
  "devDependencies": {
    "@types/node": "^22.18.12",
    "happy-dom": "^20.0.2",
    "tsc-multi": "^1.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
