{
  "name": "@anyhunt/anyhunt-server",
  "version": "0.0.1",
  "description": "Anyhunt Dev Unified API Server",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "pnpm prisma:generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "prelint": "pnpm prisma:generate",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "pretypecheck": "pnpm prisma:generate",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=16384' tsc --noEmit",
    "pretest": "pnpm prisma:generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "pretest:unit": "pnpm prisma:generate",
    "test:unit": "vitest run",
    "pretest:integration": "pnpm prisma:generate",
    "test:integration": "RUN_INTEGRATION_TESTS=1 vitest run",
    "pretest:e2e": "pnpm prisma:generate",
    "test:e2e": "RUN_INTEGRATION_TESTS=1 vitest run",
    "test:render": "vitest run",
    "test:ci": "RUN_INTEGRATION_TESTS=1 vitest run --coverage",
    "prisma:generate": "pnpm prisma:generate:main && pnpm prisma:generate:vector",
    "prisma:generate:main": "DATABASE_URL=postgresql://dummy:dummy@localhost:5432/dummy prisma generate --schema=prisma/main/schema.prisma",
    "prisma:generate:vector": "VECTOR_DATABASE_URL=postgresql://dummy:dummy@localhost:5432/dummy prisma generate --schema=prisma/vector/schema.prisma",
    "prisma:push": "pnpm prisma:push:main && pnpm prisma:push:vector",
    "prisma:push:main": "node -e \"if (process.env.NODE_ENV === 'production') { console.error('prisma db push is dev-only'); process.exit(1); }\" && prisma db push --schema=prisma/main/schema.prisma",
    "prisma:push:vector": "node -e \"if (process.env.NODE_ENV === 'production') { console.error('prisma db push is dev-only'); process.exit(1); }\" && prisma db push --schema=prisma/vector/schema.prisma",
    "prisma:migrate": "pnpm prisma:migrate:main && pnpm prisma:migrate:vector",
    "prisma:migrate:main": "prisma migrate dev --config prisma.main.config.ts",
    "prisma:migrate:vector": "prisma migrate dev --config prisma.vector.config.ts",
    "prisma:studio": "prisma studio --schema=prisma/main/schema.prisma",
    "prisma:studio:vector": "prisma studio --schema=prisma/vector/schema.prisma"
  },
  "dependencies": {
    "@anyhunt/api": "workspace:*",
    "@openai/agents-core": "0.5.1",
    "@openai/agents-extensions": "0.5.1",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.700.0",
    "@ai-sdk/anthropic": "3.0.46",
    "@ai-sdk/google": "3.0.30",
    "@ai-sdk/openai": "3.0.31",
    "@openrouter/ai-sdk-provider": "2.2.3",
    "ai": "6.0.97",
    "@mozilla/readability": "^0.6.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@scalar/nestjs-api-reference": "^1.0.13",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.4.5",
    "bullmq": "^5.66.0",
    "creem_io": "^0.4.0",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.3.3",
    "ioredis": "^5.8.2",
    "ipaddr.js": "^1.9.1",
    "jsdom": "^27.2.0",
    "mailchecker": "^6.0.19",
    "mime-types": "^3.0.2",
    "minimatch": "^10.1.1",
    "nestjs-pino": "^4.5.0",
    "openai": "6.24.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.49.0",
    "prisma": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.6.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "ws": "^8.18.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@testcontainers/postgresql": "^11.11.0",
    "@testcontainers/redis": "^11.11.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.17",
    "dotenv": "^17.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "testcontainers": "^11.11.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.15"
  }
}
