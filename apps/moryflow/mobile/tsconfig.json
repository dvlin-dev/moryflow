{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "/shared-storage/*": ["../../packages/shared-storage/src/*"],
      "/shared-types/*": ["../../packages/shared-types/src/*"],
      "/shared-core/*": ["../../packages/shared-core/src/*"],
      "/shared-types": ["../../packages/shared-types/src/index.ts"],
      "/shared-core": ["../../packages/shared-core/src/index.ts"],
      "@anyhunt/tiptap/*": ["../../packages/tiptap/src/*"],
      "@openai/agents-core/_shims": ["./src/openai-agents-core-shims.ts"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "uniwind-types.d.ts"]
}
