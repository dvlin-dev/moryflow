  "scripts": {
    "prebuild": "pnpm prisma:generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "prelint": "pnpm prisma:generate",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "pretypecheck": "pnpm prisma:generate",
    "typecheck": "tsc --noEmit",
    "pretest": "pnpm prisma:generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --pool threads",
    "pretest:e2e": "pnpm prisma:generate",
    "test:e2e": "vitest run --config ./vitest.e2e.config.ts",
    "prisma:generate": "DATABASE_URL=postgresql://dummy:dummy@localhost:5432/dummy prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
